{
  "name": "aft-ui-selenium",
  "version": "4.0.0",
  "description": "Automated Functional Testing (AFT) package supporting Selenium-based UI testing with support for BrowserStack, Sauce Labs and Local Selenium Grid testing",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --declaration",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./jasmine.json",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bicarbon8/aft-ui-selenium.git"
  },
  "keywords": [
    "aft",
    "automation",
    "e2e",
    "integration",
    "functional",
    "ui",
    "selenium",
    "browserstack",
    "saucelabs",
    "seleniumgrid"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bicarbon8/aft-ui-selenium/issues"
  },
  "homepage": "https://github.com/bicarbon8/aft-ui-selenium#readme",
  "dependencies": {
    "aft-ui": "^4.0.0",
    "es6-promise": "4.2.8",
    "selenium-webdriver": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@types/jasmine": "3.4.6",
    "@types/node": "12.12.7",
    "@types/selenium-webdriver": "4.0.5",
    "jasmine": "3.5.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "3.7.2"
  }
}
